<div class="page-header">
  <h3>User #<%= @user.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Username</dt>
      <dd><%= @user.username %></dd>
    </dl>
  </div>
</div>


 <ul class="list-group">
    <li class="list-group-item list-group-item-success">
      Photos
    </li>
    <% @users.photos do |photo| %>
    <li class="list-group-item">
      <%= photo.image %>
      </a>
    </li>
    <% end %>
</ul>


<div class="btn-group btn-group-justified">
      <a href="/users" class="btn btn-primary">
        Back
      </a>
      <a href="/photos/<%= @user.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_photo/<%= @user.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
</div>

